  locale/@AB_CD@/im/im.properties                                       (%im/im.properties)
  locale/@AB_CD@/im/imAccountSetup.dtd                                  (%im/imAccountSetup.dtd)
  locale/@AB_CD@/im/imContactDialog.dtd                                 (%im/imContactDialog.dtd)
  locale/@AB_CD@/im/csTPProtocolParams.dtd                              (%im/csTPProtocolParams.dtd)
  locale/@AB_CD@/im/csIMPreferences.dtd                                 (%im/csIMPreferences.dtd)
  locale/@AB_CD@/im/csChatHistory.dtd                                   (%im/csChatHistory.dtd)
# 
# chat
% locale chat @AB_CD@ %locale/@AB_CD@/chat/
  locale/@AB_CD@/chat/csChatAccountDialog.properties                    (%chat/csChatAccountDialog.properties)
  locale/@AB_CD@/chat/csChatAccountDialog.dtd                           (%chat/csChatAccountDialog.dtd)
  locale/@AB_CD@/chat/csChatContactDialogs.dtd                          (%chat/csChatContactDialogs.dtd)
  locale/@AB_CD@/chat/csChatOverlay.properties                          (%chat/csChatOverlay.properties)
