  locale/@AB_CD@/collab/credits.dtd                                     (%collab/credits.dtd)
  locale/@AB_CD@/collab/collab.dtd                                      (%collab/collab.dtd)
  locale/@AB_CD@/collab/tabcollab.dtd                                   (%collab/tabcollab.dtd)
  locale/@AB_CD@/collab/collab.properties                               (%collab/collab.properties)
  locale/@AB_CD@/collab/about.dtd                                       (%collab/about.dtd)
  locale/@AB_CD@/collab/commands.dtd                                    (%collab/commands.dtd)
  locale/@AB_CD@/collab/toolbars.dtd                                    (%collab/toolbars.dtd)
  locale/@AB_CD@/collab/splash.dtd                                      (%collab/splash.dtd)
  locale/@AB_CD@/collab/migration.dtd                                   (%collab/migration.dtd)
  locale/@AB_CD@/collab/migration.properties                            (%collab/migration.properties)
  locale/@AB_CD@/collab/applets.dtd                                     (%collab/applets.dtd)
  locale/@AB_CD@/collab/timezoneSelect.dtd                              (%collab/timezoneSelect.dtd)
  locale/@AB_CD@/collab/csGeneralPrefs.dtd                              (%collab/csGeneralPrefs.dtd)
  locale/@AB_CD@/collab/csDateFormatter.properties                      (%collab/csDateFormatter.properties)
