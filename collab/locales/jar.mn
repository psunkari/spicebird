#filter substitution
@AB_CD@.jar:
% locale messenger @AB_CD@ %locale/@AB_CD@/messenger/
# addressbook
  locale/@AB_CD@/messenger/addressbook/contacts.dtd                     (%addrbook/contacts.dtd)
  locale/@AB_CD@/messenger/addressbook/contacts.properties              (%addrbook/contacts.properties)
# mailnews
  locale/@AB_CD@/messenger/csRSSApplet.dtd                              (%mailnews/csRSSApplet.dtd)
  locale/@AB_CD@/messenger/csRSSApplet.properties                       (%mailnews/csRSSApplet.properties)
  locale/@AB_CD@/messenger/csMailnewsApplet.dtd                         (%mailnews/csMailnewsApplet.dtd)
  locale/@AB_CD@/messenger/csMailnewsApplet.properties                  (%mailnews/csMailnewsApplet.properties)
  locale/@AB_CD@/messenger/mailnews.dtd                                 (%mailnews/mailnews.dtd)
+ locale/@AB_CD@/messenger/start.dtd                                    (%mailnews/start.dtd)
  locale/@AB_CD@/messenger/synovel-messenger.properties                 (%mailnews/synovel-messenger.properties)
+ locale/@AB_CD@/messenger/preferences/displayOverlay.dtd               (%mailnews/preferences/displayOverlay.dtd)
#
# Changes to the global DTDs
+ locale/@AB_CD@/global/customizeToolbar.dtd                            (%collab/customizeToolbar.dtd)
#
# collab
% locale collab @AB_CD@ %locale/@AB_CD@/collab/
