[ConnectionManager]
BusName=org.freedesktop.Telepathy.ConnectionManager.haze
ObjectPath=/org/freedesktop/Telepathy/ConnectionManager/haze

[Protocol aim]
param-account=s required
param-password=s required
param-server=s
param-port=i
param-use-ssl=b
param-always-use-rv-proxy=b
param-allow-multiple-logins=b
default-server=login.messaging.aol.com
default-port=5190
default-use-ssl=false
default-always-use-rv-proxy=false
default-allow-multiple-logins=true

